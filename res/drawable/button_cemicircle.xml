<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="22.5dp"/>
            <stroke android:width="1px" android:color="@color/stokecolor"/>
            <solid android:color="@color/reservebackcolor" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="22.5dp"/>
            <stroke android:width="1px" android:color="@color/stokecolor"/>
            <gradient android:startColor="@color/commitbuttonstart" android:endColor="@color/commitbuttonend" android:angle="270" />
        </shape>
    </item>

</selector>
