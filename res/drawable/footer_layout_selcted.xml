<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape>
            <gradient android:startColor="#E1E3E6" android:endColor="#E1E3E6" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
    
	<item >
        <shape>
            <stroke android:width="1px" android:color="#D4D6D9"/>
            <gradient android:startColor="#F5F7FA" android:endColor="#F0F2F5" android:angle="270"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>

</selector>
